        <version>Winter2017-TEST-002-SNAPSHOT</version>
    </parent>

    <artifactId>edeal-admin-console</artifactId>
    <packaging>war</packaging>
    
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <executions>
                    <execution>
                        <configuration>
                            <source>1.8</source>
                            <target>1.8</target>
                            <encoding>UTF-8</encoding>
